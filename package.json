{
  "name": "v-tippy-3",
  "version": "0.1.5",
  "description": "Create tooltips and popovers with this Vue 3 directive which uses TippyJS",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.mjs",
  "umd:main": "dist/index.umd.js",
  "source": "src/index.ts",
  "typings": "types/index.d.ts",
  "repository": "https://d8vjork@github.com/d8vjork/v-tippy.git",
  "author": "RubeÃÅn Robles <d8vjork@outlook.com>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bundle": "microbundle -f es,cjs,umd",
    "watch": "bundle --watch",
    "clean": "rimraf dist",
    "build": "npm-run-all --silent clean -p bundle",
    "prepublish": "build"
  },
  "dependencies": {
    "lodash.defaultsdeep": "^4.6.1",
    "tippy.js": "^6.3.2",
    "type-fest": "^2.3.4",
    "vue": "^3.2.19"
  },
  "devDependencies": {
    "@types/lodash.defaultsdeep": "^4.6.6",
    "jest": "^27.2.4",
    "microbundle": "^0.13.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tsc": "^2.0.3",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist/*",
    "types/*"
  ]
